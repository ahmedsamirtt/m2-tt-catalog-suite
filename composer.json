{
    "name": "twentytoo/catalog-suite-services",
    "description": "A suite of services for managing catalogs in Magento 2, including similarity comparison, automatic tagging, image search, and text search.",
    "type": "magento2-module",
    "version": "1.0.0",
    "require": {
        "php": "^7.0|^8.0",
        "magento/framework": "^104|^105|^106",
        "twentytoo/similarity": "^1.0",
        "twentytoo/autotag": "^1.0",
        "twentytoo/imagesearch": "^1.0",
        "twentytoo/textsearch": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "twentytoo\\catalog-suite-services\\": ""
        }
    },
    "extra": {
        "map": [
            {
                "*": "twentytoo/similarity"
            },
            {
                "*": "twentytoo/autotag"
            },
            {
                "*": "twentytoo/imagesearch"
            },
            {
                "*": "twentytoo/textsearch"
            }
        ]
    }
}
